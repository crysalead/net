{
    "name": "crysalead/net",
    "description": "Net - Network Message implementation",
    "keywords": ["http", "psr-7"],
    "license": "MIT",
    "authors": [
        {
            "name": "CrysaLEAD"
        }
    ],
    "require": {
        "php": ">=7",
        "ext-iconv": "*",
        "ext-intl": "*",
        "crysalead/collection": "~3.0",
        "crysalead/env": "~1.0",
        "crysalead/set": "~2.0",
        "crysalead/storage-stream": "dev-master",
        "crysalead/text": "~2.0",
        "psr/http-message": "~1.0"
    },
    "require-dev": {
        "kahlan/kahlan": "~4.0",
        "crysalead/dir": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Lead\\Net\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lead\\Net\\Spec\\": "spec/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev"
}
