{
    "name": "crysalead/net",
    "description": "Net - Object-Oriented API for Network Communication",
    "keywords": ["TCP", "UDP"],
    "license": "MIT",
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "crysalead/collection": "dev-master",
        "crysalead/storage-stream": "dev-master",
        "crysalead/set": "~1.0"
    },
    "require-dev": {
        "crysalead/kahlan": "~1.2"
    },
    "autoload": {
        "psr-4": {
            "net\\": "src/",
            "net\\spec\\": "spec/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev"
}
