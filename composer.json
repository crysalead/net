{
    "name": "crysalead/net",
    "description": "Net - Object-Oriented API for low level network communication",
    "keywords": ["TCP", "UDP"],
    "license": "MIT",
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "crysalead/collection": "dev-master",
        "crysalead/storage-stream": "dev-master",
        "crysalead/set": "dev-master"
    },
    "require-dev": {
        "crysalead/kahlan": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "net\\": "src/",
            "net\\spec\\": "spec/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev"
}
